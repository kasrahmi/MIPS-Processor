module iCache_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [7:0] address;
reg clockIn;
reg clockOut;
// wires                                               
wire [15:0] instruction;

wire sampler;                             

// assign statements (if any)                          
iCache i1 (
// port map - connection between master ports and signals/registers   
	.address(address),
	.clockIn(clockIn),
	.clockOut(clockOut),
	.instruction(instruction)
);
// address[ 7 ]
initial
begin
	address[7] = 1'b0;
end 
// address[ 6 ]
initial
begin
	address[6] = 1'b0;
end 
// address[ 5 ]
initial
begin
	address[5] = 1'b0;
end 
// address[ 4 ]
initial
begin
	address[4] = 1'b0;
end 
// address[ 3 ]
initial
begin
	address[3] = 1'b0;
	address[3] = #220000 1'b1;
	address[3] = #80000 1'b0;
end 
// address[ 2 ]
initial
begin
	address[2] = 1'b0;
	address[2] = #80000 1'b1;
	address[2] = #140000 1'b0;
	address[2] = #160000 1'b1;
	address[2] = #70000 1'b0;
	address[2] = #70000 1'b1;
	address[2] = #80000 1'b0;
end 
// address[ 1 ]
initial
begin
	address[1] = 1'b1;
	address[1] = #80000 1'b0;
	address[1] = #60000 1'b1;
	address[1] = #80000 1'b0;
	address[1] = #80000 1'b1;
	address[1] = #150000 1'b0;
end 
// address[ 0 ]
initial
begin
	address[0] = 1'b0;
end 

// clockIn
initial
begin
	clockIn = 1'b1;
	# 10000;
	repeat(49)
	begin
		clockIn = 1'b0;
		clockIn = #10000 1'b1;
		# 10000;
	end
	clockIn = 1'b0;
end 

// clockOut
always
begin
	clockOut = 1'b0;
	clockOut = #10000 1'b1;
	#10000;
end 

iCache_vlg_sample_tst tb_sample (
	.address(address),
	.clockIn(clockIn),
	.clockOut(clockOut),
	.sampler_tx(sampler)
);

iCache_vlg_check_tst tb_out(
	.instruction(instruction),
	.sampler_rx(sampler)
);
endmodule

