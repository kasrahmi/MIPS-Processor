`timescale 1 ps/ 1 ps
module iCache_vlg_sample_tst(
	address,
	clockIn,
	clockOut,
	sampler_tx
);
input [7:0] address;
input  clockIn;
input  clockOut;
output sampler_tx;

reg sample;
time current_time;
always @(address or clockIn or clockOut)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

